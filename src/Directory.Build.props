<Project>
    <PropertyGroup>
        <Version>0.16.0</Version>
        <PackageVersion>0.16.0</PackageVersion>
        <Authors>Roman Vaughan,patagona</Authors>
        <Company>NZSmartie</Company>
        <Product>CoAPNet</Product>
        <PackageProjectUrl>https://github.com/patagonaa/CoAP.Net</PackageProjectUrl>
        <RepositoryUrl>https://github.com/patagonaa/CoAP.Net</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <IncludeSource>True</IncludeSource>
        <IncludeSymbols>True</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <PackageReleaseNotes>See https://github.com/patagonaa/CoAP.Net/tree/master/Changelog.md</PackageReleaseNotes>
        <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
        <PackageReadmeFile>Readme.md</PackageReadmeFile>
        <Copyright>Copyright Â© Roman Vaughan 2017</Copyright>
    </PropertyGroup>
    <ItemGroup>
        <None Include="..\..\Readme.md" Pack="true" PackagePath="\" />
    </ItemGroup>
</Project>